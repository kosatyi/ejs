{
  "name": "@kosatyi/ejs",
  "description": "EJS Templates",
  "homepage": "https://github.com/kosatyi/ejs",
  "type": "module",
  "version": "0.0.45",
  "main": "dist/cjs/ejs.js",
  "module": "dist/mjs/ejs.js",
  "browser": "dist/umd/ejs.js",
  "minified": "dist/umd/ejs.min.js",
  "author": "Stepan Kosatyi (https://kosatyi.com)",
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "exports": {
    ".": {
      "types": "./ejs.d.ts",
      "import": "./dist/mjs/ejs.js",
      "require": "./dist/cjs/ejs.js"
    },
    "./element": {
      "types": "./element.d.ts",
      "import": "./dist/mjs/element.js",
      "require": "./dist/cjs/element.js"
    }
  },
  "typesVersions": {
    "*": {
      "element": [
        "element.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "ejs.d.ts",
    "element.d.ts",
    "global.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "version": "npm run build",
    "prepublishOnly": "npm i && rollup -c",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@kosatyi/ejs-bundle": "^1.0.8",
    "@kosatyi/ejs": "file:./",
    "@kosatyi/rollup": "^0.0.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kosatyi/ejs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kosatyi/ejs.git"
  },
  "keywords": [
    "ejs",
    "template-engine",
    "nodejs"
  ]
}
